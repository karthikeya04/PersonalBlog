<%-include("./partials/header") -%>

<h1>Home</h1>
<p><%= homeContent %></p>

<% posts.forEach((x)=>{ %>
<h1><%=x.title%></h1>
<p>
  <%=x.postBody.substr(0,200)+"...."%>
  <a href="/posts/<%=x._id%>">Read More</a>
</p>
<% }) %>

<button onclick="location.href = '/compose';" class="btn btn-primary" type="submit" name="button"" >Compose</button>

<%-include("./partials/footer") -%>
